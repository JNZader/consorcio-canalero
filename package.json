{
  "name": "consorcio-canalero",
  "version": "1.0.0",
  "private": true,
  "description": "Sistema de gestion para Consorcio Canalero 10 de Mayo - Bell Ville",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/consorcio-canalero.git"
  },
  "scripts": {
    "dev": "make dev",
    "dev:frontend": "cd consorcio-web && npm run dev",
    "dev:backend": "make backend-dev",
    "build": "make build",
    "build:frontend": "cd consorcio-web && npm run build",
    "test": "make test",
    "test:frontend": "cd consorcio-web && npm run test",
    "test:backend": "make backend-test",
    "test:e2e": "cd consorcio-web && npm run test:e2e",
    "lint": "make lint",
    "lint:frontend": "cd consorcio-web && npm run lint",
    "lint:backend": "make backend-lint",
    "lint:fix": "cd consorcio-web && npm run lint:fix",
    "format": "cd consorcio-web && npm run format",
    "typecheck": "cd consorcio-web && npm run typecheck",
    "docker:dev": "docker compose up -d",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:build": "docker compose build",
    "clean": "make clean",
    "setup": "make setup",
    "prepare": "echo 'Run: make setup'"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "consorcio",
    "canalero",
    "gestion",
    "hidrico",
    "gee",
    "google-earth-engine",
    "fastapi",
    "react"
  ],
  "author": "Consorcio Canalero 10 de Mayo",
  "license": "MIT"
}
