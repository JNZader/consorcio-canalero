{
  "name": "consorcio-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test tests/accessibility/",
    "generate:types": "openapi-typescript http://localhost:8000/openapi.json -o ./src/types/schema.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@fontsource/inter": "^5.2.8",
    "@mantine/charts": "^8.3.14",
    "@mantine/core": "^8.3.11",
    "@mantine/dates": "^8.3.11",
    "@mantine/dropzone": "^8.3.11",
    "@mantine/form": "^8.3.11",
    "@mantine/hooks": "^8.3.11",
    "@mantine/notifications": "^8.3.11",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.89.0",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-router": "^1.147.0",
    "dayjs": "^1.11.19",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-draw": "^1.0.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "happy-dom": "^20.1.0",
    "openapi-typescript": "^7.10.1",
    "patch-package": "^8.0.1",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16"
  }
}
